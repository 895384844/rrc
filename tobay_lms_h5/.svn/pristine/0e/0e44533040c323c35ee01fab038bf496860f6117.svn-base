        <div class="infoBox">
            <div class="ifolist clearfix">
                <span class="fl regost active">微信绑定</span>

            </div>

            <div class="formBox" id="form_no">
                <form id="signupForm" method="post" action="">
                    <span id="phone_input">
                        <input type="text" placeholder="请输入手机号" id='phone' name="mobile" maxLength="11" value="" class='form_v'>
                    </span>
                    <span id='phoneVer_input'><input type="text" placeholder="请输入手机验证码" id='phone_verfy' class='form_v'></span>

                     <input type="button" class='getPhoneVer' id='getPhone' value="获取手机验证码">
                    <input type="button" class="usualBtn regBtn mt40 mb40" id='ptReg' value='提交'>
                </form>

            </div>
            <div class="formBox" id="form_ie">
                <form id="signupForm" method="post" action="">
                    <label for=""><em>手机号:</em>
                    <span id="phone_input">
                        <input type="text" id='phone' name="mobile" maxLength="11" value="" class='form_v'>
                    </span>
                    </label>
                    <label for=""><em>手机验证码</em><input type="button" class='getPhoneVer' id='getPhone' value="获取手机验证码"><span id='phoneVer_input'><input type="text" id='phone_verfy' class='form_v'></span></label>
                    <input type="button" class="usualBtn regBtn mt40 mb40" id='ptReg' value='提交'>
                </form>

            </div>
            
            
        </div>
